{
  "name": "@edfi/metaed-plugin-edfi-meadowlark",
  "main": "dist/index.js",
  "version": "0.2.0-dev.0",
  "description": "MetaEd plugin for generating Meadowlark object models",
  "license": "Apache-2.0",
