{
  "name": "@edfi/meadowlark-fastify",
  "version": "0.3.0-dev.0",
  "description": "Meadowlark service using Fastify",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
  "files": [
    "/dist",
    "/LICENSE.md",
    "/package.json"
  ],
  "dependencies": {
    "@edfi/meadowlark-authz-server": "0.3.0-dev.0",
    "@edfi/meadowlark-core": "0.3.0-dev.0",
    "@edfi/meadowlark-utilities": "0.3.0-dev.0",
    "@fastify/rate-limit": "^6.0.1",
    "dotenv": "^16.0.3",
    "fastify": "^3.29.3"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:copy-non-ts && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "tsc",
    "build:copy-non-ts": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" dist --verbose",
    "start:local": "node --require=dotenv/config dist/index.js",
    "start:local:ts": "ts-node --require=dotenv/config src/index.ts",
    "createKey": "openssl rand -base64 256"
  }
}
