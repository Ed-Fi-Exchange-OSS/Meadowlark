{
  "name": "@edfi/meadowlark-postgresql-backend",
  "main": "dist/index.js",
  "version": "0.2.0-dev.0",
  "description": "Meadowlark backend plugin for PostgreSQL",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
  "files": [
    "/dist",
    "/LICENSE.md",
    "/package.json"
  ],
  "scripts": {
    "build": "yarn build:clean && yarn build:copy-non-ts && yarn build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "tsc",
    "build:copy-non-ts": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" dist --verbose"
  },
  "dependencies": {
    "@edfi/meadowlark-core": "0.2.0-dev.0",
    "@edfi/meadowlark-utilities": "workspace:^",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4",
    "ramda": "^0.28.0",
    "ts-invariant": "^0.10.3"
  },
  "devDependencies": {
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2"
  }
}
