{
  "name":"kafka_opensearch",
  "config": {
    "connector.class": "io.aiven.kafka.connect.opensearch.OpensearchSinkConnector",
    "topics": "edfi.meadowlark.documents, edfi.meadowlark.authorizations",
    "connection.url": "<your-opensearch-url>",
