{
  "name": "meadowlark",
  "version": "0.1.0",
  "description": "The Ed-Fi Meadowlark project",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
  "files": [
    "/dist",
    "/CHANGELOG.md",
    "/LICENSE.md",
    "/package.json"
  ],
  "main": "handler.js",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.44.0",
    "@aws-sdk/credential-provider-node": "3.41.0",
    "@aws-sdk/lib-dynamodb": "3.44.0",
    "@aws-sdk/smithy-client": "3.41.0",
    "@aws-sdk/types": "3.40.0",
    "@aws-sdk/util-dynamodb": "3.44.0",
    "@elastic/elasticsearch": "7.13.0",
    "@hapi/joi": "^17.1.1",
    "aws4": "^1.11.0",
    "axios": "^0.25.0",
    "deep-freeze": "^0.0.1",
    "didyoumean2": "^5.0.0",
    "ed-fi-model-3.3b": "^2.0.2",
    "fast-memoize": "^2.5.2",
    "follow-redirects": "^1.14.7",
    "fs-extra": "^10.0.0",
    "inflection": "^1.13.1",
    "invariant": "^2.2.4",
    "jssha": "^3.2.0",
    "metaed-core": "^2.6.0",
    "metaed-plugin-edfi-unified": "^2.6.0",
    "njwt": "^1.2.0",
    "ramda": "^0.27.2",
    "secure-random": "^1.1.2",
    "semver": "^7.3.5",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.5",
    "@types/aws-lambda": "^8.10.89",
    "@types/eslint": "^8.2.2",
    "@types/fs-extra": "^8.1.2",
    "@types/hapi__joi": "^16.0.12",
    "@types/inflection": "^1.13.0",
    "@types/jest": "27.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "14.14.31",
    "@types/pluralize": "^0.0.29",
    "@types/ramda": "0.27.64",
    "@types/semver": "^7.3.9",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dynamodb-localhost": "^0.0.9",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-typescript": "3.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jasmine": "4.1.0",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^7.0.4",
    "jest": "27.4.5",
    "js-yaml": "^4.1.0",
    "jsinspect": "^0.12.7",
    "prettier": "2.0.2",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "serverless": "2.69.1",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-offline": "8.3.1",
    "serverless-plugin-offline-dynamodb-stream": "1.0.20",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.3",
    "ts-loader": "^6.2.2",
    "ts-node": "8.8.1",
    "typescript": "4.5.4"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:copy-non-ts && yarn build:dist",
    "build:clean": "rimraf dist",
    "build:copy-non-ts": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" dist --verbose",
    "build:dist": "tsc",
    "test": "yarn test:lint && cross-env NODE_OPTIONS=--max-old-space-size=4096 LOG_LEVEL=warn yarn jest",
    "test:lint": "yarn test:lint:ts && yarn test:lint:eslint",
    "test:lint:eslint": "eslint --max-warnings 0 --ext .js,.ts .",
    "test:lint:ts": "tsc -p . --noEmit",
    "test:unit": "cross-env NODE_OPTIONS=--max-old-space-size=4096 LOG_LEVEL=warn yarn jest --testPathIgnorePatterns ./*/integration/*",
    "test:integration": "cross-env NODE_OPTIONS=--max-old-space-size=4096 LOG_LEVEL=warn yarn jest test/integration/**",
    "test:coverage": "yarn test:unit --collectCoverage",
    "prepare": "cd .. && husky install",
    "init:local-dynamodb": "yarn serverless dynamodb install",
    "deploy:aws-dev": "yarn build && yarn serverless deploy --aws-profile default --stage dev",
    "teardown:aws-dev": "yarn serverless remove --aws-profile default --stage dev",
    "deploy:aws-stg": "yarn build && yarn serverless deploy --aws-profile default --stage stg",
    "teardown:aws-stg": "yarn serverless remove --aws-profile default --stage stg",
    "start:local": "yarn build && yarn serverless offline start --printOutput --stage local --allowCache",
    "load:descriptors:local": "yarn serverless invoke local -f loadDescriptors",
    "load:descriptors:aws-dev": "yarn serverless invoke --function loadDescriptors --stage dev"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test:lint"
    }
  },
  "packageManager": "yarn@3.1.1"
}
